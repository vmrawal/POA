<core:View
	controllerName="view.DetailNotes"
	xmlns="sap.m"
	xmlns:form="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core" >
	<List
		id="list"
		showSeparators="None"
		items="{path:'Notes'}" >
		<FeedListItem
			sender="{Author}"
			icon="{Thumbnail}"
			timestamp="{path:'CreatedAt', formatter:'util.Formatter.Date'}"
			text="{Text}" />
	</List>
</core:View>